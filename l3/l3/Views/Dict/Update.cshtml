
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Update</title>
    <style type="text/css">
        * {
            font-size: 14pt;
            margin-top: 5px;
        }

        th {
            text-align: center;
        }
    </style>
</head>
<body>
    <div>
        <h3>Обновить</h3>
        <form method="post" action="./UpdateSave">
            <input type="hidden" name="Id" value="@ViewBag.Object.Id" />
            <table>
                <tr>
                    <td><p>Фамилия</p></td>
                    <td><input type="text" name="FIO" value="@ViewBag.Object.FIO" /> </td>
                </tr>
                <tr>
                    <td><p>Телефон</p></td>
                    <td>
                        <input type="text" name="Telephone" value="@ViewBag.Object.Telephone" />
                    </td>
                </tr>
                <tr><td><input type="submit" value="Отправить" /> </td><td></td></tr>
            </table>
        </form>
    </div>
</body>
</html>